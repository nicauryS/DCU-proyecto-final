    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        String usuario = USUARIO.getText();
        String paswd = PASSWORD.getText();

        if(usuario.isEmpty() || paswd.isEmpty() ){
            JOptionpane.showMessageDialog(null,"algun vampo esta vacio ");

        }
        else {
            if(USUARIO.equals("usuario1")&& paswd.equals("Itla1234")){
                JOptionpane.showMessageDialog(null, "Bienvenido");
                Panel_de_control pc = new Panel_de_control();
                pc.setvisible(true);
                this.dispose();

            }
            else {
                JOptionpane.showMessageDialog(null, "su usuario o contraseña es incorrecto");

    }                                        

